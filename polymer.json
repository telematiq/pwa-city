{
  "entrypoint": "index.html",
  "shell": "src/news-app.html",
  "fragments": [
    "src/news-list.html",
    "src/news-article.html",
    "src/news-lazy-resources.html",
    "skin/theme-warning-path.html"
  ],
  "sources": [
    "src/**/*",
    "skin/**/*",
    "images/**/*",
    "data/**/*",
    "bower.json"
  ],
  "extraDependencies": [
    "manifest.json",
    "bower_components/webcomponentsjs/*"
  ],
  "lint": {
    "rules": ["polymer-2"]
  },
  "builds": [
    {
      "preset": "es6-unbundled"
    }
  ]
}
